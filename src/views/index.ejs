    <title>Image Processing Api</title>
  
    
  
</head>
<body>
   
    <%- include('partials/header') %>
    <div id="content">
       
    </div>
    <%- include('partials/footer') %>

</body>
<script>
     $.ajax({
      url: '/home',
      method: 'POST',
      success: function(response) {
        $('#content').html(response); // replace the #content element with the partial content
      },
      error: function(error) {
        console.error('Error loading partial:', error);
      }
    });
  $(document).ready(function() {
  $('a[data-partial]').click(function(event) {
    event.preventDefault();
    var partial = $(this).data('partial'); 
    var currentPage = '/' + partial;
    // make an AJAX request to retrieve the partial content
    $.ajax({
      url: currentPage,
      method: 'POST',
      success: function(response) {
        $('#content').html(response); // replace the #content element with the partial content
      },
      error: function(error) {
        console.error('Error loading partial:', error);
      }
    });
  });
});
</script>


</html>